[  {    "type": "post",    "url": "api/auth/employee",    "title": "员工认证页面接口",    "name": "auth_employee",    "group": "auth",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "cWorkNo",            "description": "<ul> <li>工号 (必须)</li> </ul>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "name",            "description": "<ul> <li>员工姓名的md5 (必须)</li> </ul>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "iDCard",            "description": "<ul> <li>员工身份证号码的md5 (必须)</li> </ul>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "code",            "description": "<ul> <li>微信code (必须)</li> </ul>"          }        ]      }    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 ok\n{\n\t\"status\": 0,\n\t\"errorCode\": \"Success\",\n\t\"data\": null\n}",          "type": "json"        },        {          "title": "Error-Response:",          "content": "\tHTTP/1.1 200 ok\n\t{\n \t\t\"status\": 1,\n\t\t\"errorCode\": \"AuthEorror\",\n\t\t\"data\": \"错误信息\"\n\t}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "/Users/dapan/Documents/workspace/XDAuth/src/main/java/com/auth/controller/AuthController.java",    "groupTitle": "auth"  },  {    "type": "get",    "url": "api/auth/unionid",    "title": "查询unionid是否是企业员工(其他应用后台调用)",    "name": "auth_unionid",    "group": "auth",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "unionId",            "description": "<ul> <li>unionId (必须, 其他应用后台从微信端拿到的unionid)</li> </ul>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "signature",            "description": "<ul> <li>签名 (必须, signature = MD5(appId + unionId + secret), secret请找认证服务管理员,请勿暴露secret,在请求中也不要传)</li> </ul>"          }        ]      }    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 ok\n{\n\t\"status\": 0,\n\t\"errorCode\": \"Success\",\n\t\"data\": true  // true 已验证的企业员工, false 非认证员工\n}",          "type": "json"        },        {          "title": "Error-Response:",          "content": "\tHTTP/1.1 200 ok\n\t{\n \t\t\"status\": 1,\n\t\t\"errorCode\": \"AuthEorror\",\n\t\t\"data\": \"错误信息\"\n\t}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "/Users/dapan/Documents/workspace/XDAuth/src/main/java/com/auth/controller/AuthController.java",    "groupTitle": "auth"  }]
